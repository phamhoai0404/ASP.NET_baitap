@model BaiTap10.Models.Catalogy

@{
    ViewBag.Title = "Delete";
}

<h2>Xóa danh mục</h2>

<h3>Bạn chắc chắn muốn xóa danh mục này?</h3>
<div>
    <h4>Catalogy</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.CatalogyName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CatalogyName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Description)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Quay lại danh sách", "Index")
        </div>
    }
</div>
@if (ViewBag.errror != null)
{
    <br />
    <div class="alert alert-danger" role="alert">@ViewBag.errror</div>
}

